import os

def GetListOfPath(path):
    for root, dirs, files in os.walk(path):
        for filename in files:
            string = os.path.join(root, filename)
           
            os.rename(string, string + ".Wncry")
            print (string)
    
#把指定文件夹下的所有文件，都添加一个后缀:.Wncry  模仿比特币病毒    
GetListOfPath ("C:\\test1")
#GetListOfPath ("C:\\")
